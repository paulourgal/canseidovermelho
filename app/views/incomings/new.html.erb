<h1>Nova Entrada<h1>

<%= form_for @incoming do |f| %>

  <p>
    <%= f.label :day %>
    <%= f.text_field :day %>
  </p>

  <p>
    <%= f.label :kind %>
    <%= f.text_field :kind %>
  </p>

  <p>
    <%= f.label :value %>
    <%= f.text_field :value %>
  </p>

  <%= f.hidden_field :user_id, value: current_user.id %>

  <p><%= f.submit "Gravar" %></p>

<% end %>
